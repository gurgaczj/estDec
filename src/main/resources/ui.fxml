<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="rootPane" prefHeight="510.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="com.gurgaczj.controller.UIController">
  <children>
    <TableView id="frequentItemsetTable" fx:id="fiTable" layoutX="0.0" layoutY="0.0" prefHeight="350.0" prefWidth="600.0">
      <columns>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="280.0" text="Zbiór częsty" fx:id="fiColumn" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="209.0" text="Wsparcie" fx:id="fiSupportColumn" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="109.0" text="Błąd" fx:id="fiErrorColumn" />
      </columns>
    </TableView>
    <TextField fx:id="bProperty" layoutX="14.0" layoutY="360.0" prefWidth="147.0" promptText="Podaj parametr b" />
    <TextField fx:id="hProperty" layoutX="14.0" layoutY="390.0" prefWidth="147.0" promptText="Podaj parametr h" />
    <TextField fx:id="sMinProperty" layoutX="14.0" layoutY="420.0" prefWidth="147.0" promptText="Podaj sMin" />
    <Button fx:id="selectFileButton" layoutX="177.0" layoutY="360.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="147.0" text="Wybierz plik .csv" />
    <TextField id="percentage" fx:id="sInsProperty" layoutX="14.0" layoutY="450.0" prefWidth="147.0" promptText="Podaj sIns" />
    <Button fx:id="mineFIButton" layoutX="331.0" layoutY="360.0" mnemonicParsing="false" onAction="#mineFis" prefHeight="25.0" prefWidth="147.0" text="Generuj zbiory częste" />
    <TextArea fx:id="logArea" layoutX="177.0" layoutY="403.0" prefHeight="83.0" prefWidth="409.0" wrapText="true" />
    <Button fx:id="clearLogsButton" layoutX="487.0" layoutY="360.0" mnemonicParsing="false" onAction="#clearLogs" prefHeight="25.0" prefWidth="99.0" text="Czyść logi" />
    <TextField id="percentage" fx:id="sPrnProperty" layoutX="14.0" layoutY="480.0" prefWidth="147.0" promptText="Podaj sPrn" />
  </children>
</AnchorPane>
